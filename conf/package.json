{
  "name": "webpack-dev-boilerplate",
  "version": "2.0.0",
  "description": "boilerplate",
  "main": "app/index.js",
  "scripts": {
    "start:pre":"webpack --config conf/webpack.dll.config.js",
    "start": "cross-env NODE_ENV=development node conf/server.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --config conf/webpack.prod.config.js",
    "lint": "eslint app",
    "clean": "rm -rf public/*",
    "test": "mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuoYongfeng/webpack-dev-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "guoyongfeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GuoYongfeng/webpack-dev-boilerplate/issues"
  },
  "homepage": "https://github.com/GuoYongfeng/webpack-dev-boilerplate#readme",
  "dependencies": {
    "history": "^4.4.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.18.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
